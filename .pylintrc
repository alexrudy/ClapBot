[TYPECHECK]

ignored-classes=SQLAlchemy, SQLObject, optparse.Values, thread._local, _thread._local,
    PurePath, scoped_session, Flask

[BASIC]
max-line-length=120

[MESSAGES CONTROL]
disable=missing-docstring,invalid-name,locally-disabled,
    locally-enabled,file-ignored,fixme,
    missing-docstring,no-self-use,too-few-public-methods

[VARIABLES]
good-names=i,j,k,ex,Run,df,dt,X,y,sample_X,sample_Y